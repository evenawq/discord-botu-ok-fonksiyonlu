# alinti komutunu tanÄ±mlayÄ±n
@bot.command()
async def alinti(ctx, konu: str):
    konu = konu.lower()
    if konu in quotes:
        alinti = random.choice(quotes[konu])
        await ctx.send(alinti)
    else:
        await ctx.send(f"{konu} hakkÄ±nda alÄ±ntÄ± bulunamadÄ±. Mevcut konular: {', '.join(quotes.keys())}")

