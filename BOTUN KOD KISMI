@bot.command()
async def rastgele(ctx):
    """Sunucudaki rastgele bir kullanÄ±cÄ±yÄ± seÃ§er."""
    if len(ctx.guild.members) > 1:
        uye = random.choice(ctx.guild.members)
        await ctx.send(f"Rastgele seÃ§ilen kullanÄ±cÄ±: {uye.mention}")
    else:
        await ctx.send("Sunucuda yeterli Ã¼ye bulunmuyor.")

data_file = 'points.json'

def load_points():
    """PuanlarÄ± dosyadan yÃ¼kler."""
    try:
        with open(data_file, 'r') as f:
            return json.load(f)
    except FileNotFoundError:
        return {}

def save_points(points):
    """PuanlarÄ± dosyaya kaydeder."""
    with open(data_file, 'w') as f:
        json.dump(points, f)

@bot.command()
async def puanver(ctx, miktar: int, uye: discord.Member):
    """Belirli bir Ã¼yeye puan verir."""
    points = load_points()
    
    if str(uye.id) not in points:
        points[str(uye.id)] = 0
    
    points[str(uye.id)] += miktar
    save_points(points)
    await ctx.send(f"{uye.mention} kullanÄ±cÄ±sÄ±na {miktar} puan verildi!")

@bot.command()
async def puan(ctx, uye: discord.Member = None):
    """KullanÄ±cÄ±nÄ±n puanÄ±nÄ± gÃ¶sterir. VarsayÄ±lan olarak kendi puanÄ±nÄ±zÄ± gÃ¶sterir."""
    points = load_points()
    user_id = str(uye.id) if uye else str(ctx.author.id)
    
    if user_id in points:
        await ctx.send(f"{ctx.guild.get_member(int(user_id)).mention}'Ä±n puanÄ±: {points[user_id]}")
    else:
        await ctx.send("Bu kullanÄ±cÄ± iÃ§in puan bulunamadÄ±.")

